{
  "name": "react-foundation",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./src/index.html --no-autoinstall",
    "start:clean": "rm -rf .cache && npm run start",
    "build": "parcel build ./src/index.html --no-source-maps --no-cache && cp .htaccess dist/.htaccess",
    "lint": "tsc && eslint . --ext .ts,.tsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint --",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.11.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "autoprefixer": "^9.7.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "sass": "^1.23.1",
    "typescript": "^3.6.4"
  }
}
